{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": {
        "type": "swc",
        "options": {
            "extensions": [".js", ".ts", ".jsx", ".tsx"]
          }
    }
  }
}
